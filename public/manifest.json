{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "activeTab",
    "tabGroups",
    "tabs",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}